<Application x:Class="Launcher.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Launcher"
             StartupUri="/View/Windows/MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary
                    Source="/View/Styles/StyleMainView.xaml"/>
                <ResourceDictionary
                    Source="/View/Styles/StyleButton.xaml"/>
                <ResourceDictionary
                    Source="/View/Styles/TopMenuBorder.xaml"/>
                <ResourceDictionary
                    Source="/View/Styles/Colors.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
